<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--Body-->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="350dp"
        android:layout_height="400dp"
        android:background="@drawable/avisosemendereco"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <!--Text Tittle-->
        <TextView
            android:id="@+id/txttittlenoadressregister"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/txtavisosemendereco"
            android:textSize="20sp"
            android:textColor="#000"
            android:textStyle="bold"
            android:layout_marginTop="8dp"
            android:textAlignment="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <!--Text Desc-->
        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="16dp"
            android:text="@string/descnenhumendencontrado"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txttittlenoadressregister" />

        <!--Animation-->
        <com.airbnb.lottie.LottieAnimationView
            android:layout_width="match_parent"
            android:layout_height="150dp"
            app:lottie_autoPlay="true"
            android:layout_marginTop="2dp"
            app:lottie_rawRes="@raw/deliveryaddress"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <!--Constraint Btn Register Now-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btncadastraragoraend"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:background="@drawable/btncadastraragoraend"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" >

            <!--Text Register Now-->
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="@string/cadastraragora"
                android:textAlignment="center"
                android:textStyle="bold"
                android:textColor="#AD000000"
                android:textSize="16sp"
                android:layout_marginEnd="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <!--Constraint Btn Register Latter-->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btncadastrardepoisend"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:background="@drawable/btncadastrardepoisend"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <!--Text Register Latter-->
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="@string/cadastrardepois"
                android:textAlignment="center"
                android:textStyle="bold"
                android:textColor="#AD000000"
                android:textSize="16sp"
                android:layout_marginStart="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>